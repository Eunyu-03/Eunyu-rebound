<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rebound.mapper.InquiryMapper">
    <insert id="insertInquiry">
        insert into tbl_inquiry(inquiry_title,inquiry_content,inquiry_email,member_id)
        values (#{inquiryTitle},#{inquiryContent},#{inquiryEmail},#{memberId})
    </insert>
</mapper>